vec4 func(int a)
{
    // Always true.
    if(ARR[a] > _int_0 && ARR[a] < _int_2)
    {
        // b becomes 1.
        int b = max(clamp(ARR[a], ARR[a], _int_1), ARR[a]);
        return vec4(b, _int_0, _int_0, _int_1);
    }
    // Never executed.
    return vec4(_int_0);
}

void main()
{
    _GLF_color = func(int(gl_FragCoord.y));
